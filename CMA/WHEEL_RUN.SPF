PROC WHEEL_RUN(REAL SPEED) DISPLOF
;;**********程序功能**********
;;砂轮启动
;SPEED砂轮线速度
DEF REAL DR0,DR1
DR0=SPEED*60000/($PI*DRESSER[1]);依据线速度计算砂轮转速

IF (INI[36]==0) OR (INI[36]==2);砂轮类型选择
    R299=SPEED
    G4 F0.2
    M13;砂轮启动
ELSE
    M1=3 S1=SPEED
ENDIF

IF $A_DBB[13]==0;修整轮当前未启动
    DR1=5;静止状态启动延时
ELSE
    DR1=1;启动状态变速延时
ENDIF
G4 F=DR1;砂轮转速等待时间
RET